{
	"name": "keystone",
	"root": "apps/keystone",
	"sourceRoot": "apps/keystone",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@ausspeedruns.com/keystone-plugin:build",
			"options": {
				"root": "apps/keystone",
				"outputPath": "dist/apps/keystone",
				"filesToInclude": ["src"]
			}
		},
		"serve": {
			"executor": "nx:run-commands",
			"options": {
				"cwd": "apps/keystone",
				"commands": [
					"npx azurite-blob -l ./azurite --blobPort 9999",
					"npx keystone dev"
				],
				"parallel": true
			},
			"configurations": {
				"production": {
					"command": "npx keystone start"
				}
			}
		},
		"container": {
			"executor": "@nx-tools/nx-container:build",
			"options": {
				"engine": "docker",
				"tags": [
					"ausrunswebsitecontainersregistry.azurecr.io/ausruns/keystone:latest"
				]
			},
			"dependsOn": ["build"]
		},
		"container-push": {
			"executor": "@nx-tools/nx-container:build",
			"options": {
				"engine": "docker",
				"push": true,
				"tags": [
					"ausrunswebsitecontainersregistry.azurecr.io/ausruns/keystone:latest"
				]
			},
			"dependsOn": ["build"]
		},
		"seed": {
			"command": "npx keystone --seed-data"
		}
	},
	"tags": []
}
