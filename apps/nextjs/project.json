{
	"name": "nextjs",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/nextjs",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nx/next:build",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"outputPath": "dist/apps/nextjs"
			},
			"configurations": {
				"development": {
					"outputPath": "apps/nextjs"
				},
				"production": {}
			}
		},
		"serve": {
			"executor": "@nx/next:server",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "nextjs:build",
				"dev": true
			},
			"configurations": {
				"development": {
					"buildTarget": "nextjs:build:development",
					"dev": true,
				},
				"production": {
					"buildTarget": "nextjs:build:production",
					"dev": false
				}
			}
		},
		"export": {
			"executor": "@nx/next:export",
			"options": {
				"buildTarget": "nextjs:build:production"
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/nextjs/jest.config.ts"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"]
		},
		"container": {
			"executor": "@nx-tools/nx-container:build",
			"options": {
				"tags": ["ausrunswebsitecontainersregistry.azurecr.io/ausruns/next:latest"],
				"file": "./apps/nextjs/Dockerfile"
			},
			"dependsOn": ["build"]
		},
		"container-push": {
			"executor": "@nx-tools/nx-container:build",
			"options": {
				"push": true,
				"tags": ["ausrunswebsitecontainersregistry.azurecr.io/ausruns/next:latest"],
				"file": "./apps/nextjs/Dockerfile"
			},
			"dependsOn": ["build"]
		}
	},
	"tags": []
}
